	<queryString><![CDATA[SELECT MASCARACFOP(CFOP) AS Mascara, CFOP, SUM(ValorTotal) as ValorContabil,
  SUM(BaseIPI) as Base, 
  SUM(ValorIPI) as ValorCredito,
  SUM(IsentoIPI) as ValorIsento,
  SUM(OutrosIPI) as ValorOutras
                      
FROM lbr_SitIPI_v

WHERE DataEntrada BETWEEN $P{STARTDATE} AND $P{ENDDATE}

  AND (CFOP LIKE '1%'
  OR CFOP LIKE '2%'
  OR CFOP LIKE '3%')
  AND IsCancelled                        ='N'
  AND AD_Client_ID = 2000000

GROUP BY CFOP
ORDER BY 2]]></queryString>

